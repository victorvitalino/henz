<div class="col-md-12">
  <div class="panel panel-default">
    <div class="panel-body">
      <%= simple_form_for(@desk_supply) do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">
        <%= f.input :name %>
        <%= f.input :address %>
        <%= f.input :telphone %>
        <%= f.input :geolocation %>
        <%= f.association :city %>
        <%= f.association :state %>
      </div>

        <div class="form-actions">
        <%= f.submit 'Salvar', class: 'btn btn-primary pull-right' %>

        <%= link_to 'Cancelar', desk_supplies_path, class: 'btn btn-danger pull-left' %>
      </div>
      <% end %>
    </div>
  </div>
</div>